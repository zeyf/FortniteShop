CurrentShop()\r\n        // eslint-disable-next-line\r\n    }, [])\r\n\r\n    \r\n\r\n\r\n    return (\r\n        <div className='shop shop--primary'>\r\n            <h1 className='shopview__head'>Today's Shop</h1>\r\n            <div className=\"shopview shopview--primary\">\r\n                {sContext.loading ? <Spinner /> :\r\n                    <>    \r\n                        <DailyShopSection />\r\n                        <FeaturedShopSection />\r\n                    </>\r\n                }\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Shop\r\n","import React from 'react';\r\nimport './Navbar.css';\r\nimport {Link} from 'react-router-dom';\r\n\r\nconst Navbar = (props) => {\r\n    return (\r\n        <div className='navbar navbar--primary'>\r\n            <Link to='/' style={{margin: '0px', padding: '0px', textDecoration: 'none'}}>\r\n                <p style={{fontFamily: 'Fortnite', color: 'white', fontSize: '2.5em',\r\n                        padding: '0px', margin: '0px'}}>FORTNITEBR</p>\r\n            </Link>\r\n            <ul className='navbarmenu navbarmenu--primary'>\r\n                <li className=\"navbarmenu__item\">\r\n                    <button className='navbarmenu__button'>\r\n                        CHECK YOUR STATS\r\n                        </button>\r\n                </li>\r\n            </ul>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Navbar\r\n","import React, {useContext} from 'react';\r\nimport ItemContext from '../../../../context/ItemContext/ItemContext';\r\nimport './ItemShopHistory.css';\r\n\r\nconst ItemShopHistory = () => {\r\n\r\n    const iContext = useContext(ItemContext)\r\n\r\n    const mutableShopHistory = () => {\r\n        if (iContext.loading === false) {\r\n\r\n            const {item} = iContext;\r\n            const {shopHistory} = item;\r\n            return [...shopHistory]\r\n\r\n            // a practical use for the spread operator...\r\n            // couldn't do array reverse (to have recent appearances first)\r\n            // on shophistory from iContext directly as it references state (which is immutable)\r\n        }\r\n    }\r\n\r\n    return (\r\n        <div className=\"itemshophistory itemshophistory--primary\">\r\n\r\n            <table className='historytable historytable--primary'>\r\n                <thead>\r\n                    <tr>\r\n                        <th scope='col'>\r\n                            Date\r\n                        </th>\r\n                        <th scope='col'>\r\n                            Price\r\n                        </th>\r\n                    </tr>\r\n                </thead>\r\n                <tbody>\r\n                    {iContext.item && mutableShopHistory().reverse().map((appearance, i) => {\r\n                        \r\n                        const dateparsed = appearance.split('T')[0];\r\n        \r\n                        return <tr className='historytable__row'>\r\n                                    <td className='historytable__data'>\r\n                                        {dateparsed}\r\n                                    </td>\r\n                                    <td className='historytable__data'>\r\n                                        dddddd\r\n                                    </td>\r\n                                </tr>\r\n                    })}\r\n                </tbody>\r\n            </table>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default ItemShopHistory\r\n","import React, {useContext} from 'react';\r\nimport ItemContext from '../../context/ItemContext/ItemContext';\r\nimport ItemShopHistory from './Item Details Components/Item Shop History/ItemShopHistory'\r\nimport './ItemDetailsSection.css';\r\n\r\nconst ItemDetailsSection = () => {\r\n\r\n    const iContext = useContext(ItemContext)\r\n    return (\r\n        <div className='itemdetailssection itemdetailssection--primary'>\r\n            <ItemShopHistory />\r\n        </div>